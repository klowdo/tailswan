# TailSwan Environment Variables
# Copy this file to .env and fill in your values

# Tailscale authentication key
# Get this from: https://login.tailscale.com/admin/settings/keys
TS_AUTHKEY=tskey-auth-xxxxxxxxxxxx-yyyyyyyyyyyyyyyyyyyyyyyyyyyy

# Subnets to advertise to Tailscale (comma-separated)
# These should match the subnets in your swanctl.conf
TS_ROUTES=10.1.0.0/24,10.2.0.0/24

# Additional Tailscale arguments (optional)
# TS_EXTRA_ARGS=--accept-routes

# Auto-start IPsec connections (optional)
# SWAN_AUTO_START=true
# SWAN_CONNECTIONS=site-to-site,remote-office
